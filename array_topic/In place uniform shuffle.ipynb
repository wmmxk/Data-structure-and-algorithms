   "outputs": [],
   "source": [
    "import random\n",
    "random.seed()\n",
    " \n",
    "def shuffle(myList):\n",
    "   n = len(myList)\n",
    "   for i in xrange(0, n):\n",
    "      j = random.randint(i, n-1) # randint is inclusive\n",
    "      myList[i], myList[j] = myList[j], myList[i]"
   ]
  }
